{
  "version": 2,
  "builds": [
    {
      "src": "src/index.ts",
      "use": "@vercel/node",
      "config": {
        "nodeVersion": "20.x"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/src/index.ts"
    }
  ],
  "build": {
    "env": {
      "CI": "false"
    }
  },
  "regions": ["iad1"]
}
